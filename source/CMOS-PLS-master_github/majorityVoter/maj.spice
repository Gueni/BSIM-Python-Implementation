.subckt MAJ180nm INPUT_0 INPUT_1 INPUT_2 GATE_5 VSS VDD

* area proportionally partioned to every involved gate
.param PsubNwellJunctionS_MAJ = '(15.6 * 5.7)/6'
.csparam PsubNwellJunctionS_MAJ = {PsubNwellJunctionS_MAJ}

XDiodePN1 VSS VDD LaserTrig SUBCKT_Iph_Psub_Nwell transConductance = a3 constCurrent = b3 distance = distGate1Top PNArea = 'PsubNwellJunctionS_MAJ'

XDiodePN2 VSS VDD LaserTrig SUBCKT_Iph_Psub_Nwell transConductance = a3 constCurrent = b3 distance = distGate2Top PNArea = 'PsubNwellJunctionS_MAJ'

XDiodePN3 VSS VDD LaserTrig SUBCKT_Iph_Psub_Nwell transConductance = a3 constCurrent = b3 distance = distGate3Top PNArea = 'PsubNwellJunctionS_MAJ'

XDiodePN4 VSS VDD LaserTrig SUBCKT_Iph_Psub_Nwell transConductance = a3 constCurrent = b3 distance = distGate4Top PNArea = 'PsubNwellJunctionS_MAJ'

XDiodePN5 VSS VDD LaserTrig SUBCKT_Iph_Psub_Nwell transConductance = a3 constCurrent = b3 distance = distGate5Top PNArea = 'PsubNwellJunctionS_MAJ'

XDiodePN6 VSS VDD LaserTrig SUBCKT_Iph_Psub_Nwell transConductance = a3 constCurrent = b3 distance = distGate6Top PNArea = 'PsubNwellJunctionS_MAJ'

R_VSS VSS NAND2X1_3/VSS 0.001
R_VDD VDD NAND2X1_3/VDD 0.001

NAND2X1 Y VSS A B VDD beamDistanceTop = 0 beamDistanceBot = 0

XNAND2X1_6 GATE_5 NAND2X1_3/VSS NAND2X1_3_Y NAND2X1_5_Y NAND2X1_3/VDD
+ NAND2X1 beamDistanceTop = distGate6Top beamDistanceBot = distGate6Bot

XNAND2X1_5 NAND2X1_5_Y NAND2X1_3/VSS NAND2X1_4_Y NAND2X1_4_Y NAND2X1_3/VDD
+ NAND2X1 beamDistanceTop = distGate5Top beamDistanceBot = distGate5Bot

XNAND2X1_4 NAND2X1_4_Y NAND2X1_3/VSS NAND2X1_2_Y NAND2X1_1_Y NAND2X1_3/VDD
+ NAND2X1 beamDistanceTop = distGate4Top beamDistanceBot = distGate4Bot

XNAND2X1_1 NAND2X1_1_Y NAND2X1_3/VSS INPUT_2 INPUT_0 NAND2X1_3/VDD
+ NAND2X1 beamDistanceTop = distGate1Top beamDistanceBot = distGate1Bot

XNAND2X1_2 NAND2X1_2_Y NAND2X1_3/VSS INPUT_1 INPUT_0 NAND2X1_3/VDD
+ NAND2X1 beamDistanceTop = distGate2Top beamDistanceBot = distGate2Bot

XNAND2X1_3 NAND2X1_3_Y NAND2X1_3/VSS INPUT_2 INPUT_1 NAND2X1_3/VDD
+ NAND2X1 beamDistanceTop = distGate3Top beamDistanceBot = distGate3Bot

C0 NAND2X1_4_Y NAND2X1_5_Y 0.14fF
C1 NAND2X1_1_Y NAND2X1_3/VDD 0.03fF
C2 NAND2X1_3/VSS NAND2X1_5_Y 0.12fF
C3 NAND2X1_2_Y XNAND2X1_1.MIDDLE 0.00fF
C4 NAND2X1_2_Y NAND2X1_1_Y 0.15fF
C5 NAND2X1_2_Y NAND2X1_3_Y 0.01fF
C6 INPUT_0 NAND2X1_1_Y 0.18fF
C7 NAND2X1_3_Y INPUT_0 0.15fF
